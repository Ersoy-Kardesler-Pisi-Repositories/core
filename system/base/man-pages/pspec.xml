    <Package>
        <Name>man-pages</Name>
        <RuntimeDependencies>
            <Dependency>man-db</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="man">/usr/share/man</Path>
